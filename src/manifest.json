{
    "name": "Osyaburi",
    "version": "1.0.0",
    "manifest_version": 2,
    "description": "Notify you of the username of the user you specified, if any, attending the connpass event.",
    "content_scripts": [
        {
            "matches": ["*://*.connpass.com/event/*/participation/*"],
            "js": ["content.js"]
        }
    ],
    "icons": {
        "16": "icon/icon16.png",
        "48": "icon/icon48.png",
        "128": "icon/icon128.png"
    }
}
